<p id="notice"><%= notice %></p>

<h1>Listing Listings</h1>

<table>
  <thead>
    <tr>
      <th>Dorm</th>
      <th>Gender</th>
      <th>Room</th>
      <th>Start date</th>
      <th>End date</th>
      <th>Description</th>
      <th>Is available</th>
      <th>Catalog</th>
      <th>Landlord</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @listings.each do |listing| %>
      <tr>
        <td><%= listing.dorm %></td>
        <td><%= listing.gender %></td>
        <td><%= listing.room %></td>
        <td><%= listing.start_date %></td>
        <td><%= listing.end_date %></td>
        <td><%= listing.description %></td>
        <td><%= listing.is_available %></td>
        <td><%= listing.catalog %></td>
        <td><%= listing.landlord %></td>
        <td><%= link_to 'Show', listing %></td>
        <td><%= link_to 'Edit', edit_listing_path(listing) %></td>
        <td><%= link_to 'Destroy', listing, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Listing', new_listing_path %>
